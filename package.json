{
  "name": "medicus",
  "private": true,
  "description": "Frontend for Medicus REST API for submission in HackerEarth's Hacking Homelessness hackathon. A web based solution for improvising the management of transitional care provided to the homeless in shelter homes.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 8080",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "rm -rf .cache public && npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.5.2",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.2.1",
    "axios": "^0.21.1",
    "emotion-theming": "^10.0.27",
    "eslint-config-react-app": "^5.1.0",
    "gatsby": "^2.18.12",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.9",
    "install": "^0.13.0",
    "npm": "^6.13.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.4.5",
    "react-icons": "^3.8.0",
    "simple-react-validator": "^1.4.1"
  },
  "devDependencies": {
    "gatsby-plugin-eslint": "^2.0.8",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydattc/medicus"
  },
  "bugs": {
    "url": "https://github.com/jaydattc/medicus"
  }
